FORCE_DEVELOPMENT_MODE=true

# LLM API Keys for AI Agent functionality
OPENAI_API_KEY=your_openai_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Development settings
NODE_ENV=development
PORT=3000

# Database (SQLite for development)
DATABASE_URL=file:./dev.db

# Redis for development (using local Redis or mock)
REDIS_URL=redis://localhost:6379

# Session security (development keys - change for production)
SESSION_SECRET=dev_session_secret_change_in_production
CSRF_SECRET=dev_csrf_secret_change_in_production
JWT_SECRET=dev_jwt_secret_change_in_production

# VNC Configuration for live browser streaming
VNC_DISPLAY=:99
VNC_PORT=5901
VNC_HOST=127.0.0.1

# Frontend URL for CORS
FRONTEND_URL=http://localhost:3000
VNC_PORT=5900
VNC_PASSWORD=vncpass123

