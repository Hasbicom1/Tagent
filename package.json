  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "vite build",
    "build-worker": "echo 'Worker is Python-based, skipping Node.js worker build'",
    "build-full": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "node server/production.js",
    "start-test": "node test-server.js",
    "start-original": "cross-env NODE_ENV=production node dist/index.js",
    "test-start": "node -e \"const http = require('http'); const server = http.createServer((req,res)=>res.end('OK')); server.listen(process.env.PORT||5000, '0.0.0.0', ()=>console.log('Test server started'));\"",
    "test-start-health": "node -e \"const http = require('http'); const server = http.createServer((req,res)=>{if(req.url==='/health'){res.writeHead(200,{'Content-Type':'application/json'});res.end(JSON.stringify({status:'healthy',timestamp:new Date().toISOString()}));}else{res.end('OK');}}); server.listen(process.env.PORT||5000, '0.0.0.0', ()=>console.log('Test server with health check started'));\"",
    "test-minimal": "node test-server.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "postinstall": "npx playwright install chromium"
  },
